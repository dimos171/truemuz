{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1 }],
    "semi": ["error", "always", { "omitLastInOneLineBlock": true }]
  }
}
